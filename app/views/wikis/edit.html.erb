<h1>Edit Wiki</h1>

<div class="row">
  <div class="col-md-4">
    <% if current_user.admin? || current_user.premium? || @wiki.users.include?(current_user) %>
      <%= render partial: 'collaborators/collaborator_form' %>
    <% end %>
  </div>
  <div class="col-md-8">
    <%= render partial: 'form' %>
  </div>
</div>
